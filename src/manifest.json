{
  "manifest_version": 2,
  "name": "Shift-Ctrl-F",
  "short_name": "Shift-Ctrl-F",
  "author": "Yoav Zimmerman <yoavzimmerman@gmail.com>",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+Ctrl+F",
        "mac": "Shift+Ctrl+F"
      }
    }
  }
}
